					<string>function run(input, parameters) {		var app = Application.currentApplication()	app.includeStandardAdditions = true	var evmdisText = input.filter(e =&gt; e.length &gt; 0).join('\n')	var result = app.displayDialog(evmdisText, {		buttons: ['Cancel', 'Copy'],		defaultButton: 'Copy',		closeButton: 'Cancel'	})	if (result) {		app.setTheClipboardTo(evmdisText)		app.displayNotification('Copied to the clipboard', {			withTitle: 'EVM disassembler',			soundName: 'Pop'		})	}	return input;}</string>
