				<key>ActionName</key>
				<string>Run JavaScript</string>
				<key>ActionParameters</key>
				<dict>
					<key>source</key>
					<string>function run(input, parameters) {	var app = Application.currentApplication()	app.includeStandardAdditions = true	var sanitizedInput = input.filter(e =&gt; e.length &gt; 0).join('\n')		sanitizedInput = sanitizedInput.replace(/(\s|0x)/gi, '');		if (!/^[0-9a-zA-Z]+$/.test(sanitizedInput)) {		app.displayNotification('Error parsing bytecode. Please select only hex data.', {			withTitle: 'EVM disassembler',			soundName: 'bass'		})	}	return input;}</string>
